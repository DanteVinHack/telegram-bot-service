{
  "name": "server-telegram-bot-vue",
  "version": "1.0.0",
  "description": "This server created that work with telegram-bot",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
		"dev": "concurrently --raw \"npm run server\" \"npm run client\"",
    "client": "npm run serve --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client"
  },
  "keywords": [
    "nodejs",
    "node-telegram-bot-api",
    "mongodb",
    "express",
    "websocket"
  ],
  "author": "Dante <VentilyatorTag@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20"
  },
  "dependencies": {
    "@qiwi/bill-payments-node-js-sdk": "^3.2.1",
    "config": "^3.3.8",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "mongoose": "^6.6.5",
    "morgan": "^1.10.0",
    "node-telegram-bot-api": "^0.59.0",
    "qiwi-sdk": "^4.0.4",
    "uuid": "^9.0.0",
    "websocket": "^1.0.34"
  }
}
